            window.onscroll = function (oEvent) {
              var mydivpostop = document.getElementById("passou_o_botao").offsetTop;
              var mydivposdown = document.getElementById("passou_a_descricao").offsetTop;
              var scrollPos = document.documentElement.scrollTop;
              if(scrollPos > (mydivpostop+350) && scrollPos < mydivposdown-600) {
                document.getElementById("botaoflutuante").className = "botaoflutuante";
              } else {
                document.getElementById("botaoflutuante").className = "hidden";
              }
            };
            
